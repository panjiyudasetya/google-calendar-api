{
    "name": "google-calendar-api",
    "version": "0.0.1",
    "description": "Google calendar api that can be used on both React and React Native project",
    "main": "dist/index",
    "typings": "dist/index",
    "files": [
        "dist"
    ],
    "scripts": {
        "postinstall": "tsc --sourceMap -p . || true",
        "build": "tsc",
        "lint": "tsc -p . --noEmit;tslint -p .",
        "tslint": "tslint \"src/**/*.{ts}\"",
        "tslint-check": "tslint-config-prettier-check ./tslint.json"
    },
    "private": true,
    "dependencies": {
        "typescript": "^2.4.2"
    },
    "devDependencies": {
        "prettier": "1.14.3",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0"
    }
}
